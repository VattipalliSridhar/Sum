    <string name="home_page">

        <![CDATA[
        <html>
            <head>
            <style type=\"text/css\">
            @font-face {font-family: MyFont;src: url(\"file:///android_asset/font/mallanna.ttf\")}
            body
            {font-family: MyFont;font-size: medium;text-align: justify;}
